fun eqb (x: bool) (y: bool): bool =
  (! ((x || y)) || (x && y))
fun reshape_int (l: int) (x: []int): []int =
  let roundUp = ((l + ((shape (x))[0] - 1)) / (shape (x))[0]) in
  let extend = reshape ((((shape (x))[0] * roundUp))) (replicate (roundUp) (x)) in
  let (v1, _) = split (l) (extend) in
  v1
